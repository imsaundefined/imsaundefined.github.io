                        $( document ).ready(function(){
                                var frontcoverCount = 12;
                                $("#frontcover").css({"background-image": "url(\"/clubhub/media/frontcover/Academy"+Math.floor((Math.random()*frontcoverCount))+".jpg\")"});
                                var height = $( window ).height();
                                var width = $( window ).width();
                                if(height > width)
                                {
                                        $("#frontcover").css({"background-size": "auto "+height+"px"});
                                        console.log("auto "+height+"px");
                                }
                                if(!mobile)
                                {
                                        $("#frontcover").animate({"opacity":"1"}, {duration: 2000, queue: false});
                                        if(height>width)
                                        { 
                                                $("#frontcover").animate({"background-size": "auto "+height*1.1+"px"}, {duration: 3000, queue: false});
                                        }
                                        else
                                        {
                                                $("#frontcover").animate({"background-size": "110%"}, {duration: 3000, queue: false});
                                        }
                                        //$("#backcover h1").animate({"background-color": "rgba(0,0,0,0.3)"}, 1000);
                                        $({blurRadius: 0}).animate({blurRadius: 5}, {
                                                duration: 3000,
                                                easing: 'linear', // or "linear"
                                                // use jQuery UI or Easing plugin for more options
                                                step: function() {
                                                        //console.log(this.blurRadius);
                                                        $('#frontcover').css({
                                                                "-webkit-filter": "blur("+this.blurRadius+"px)",
                                                                "filter": "blur("+this.blurRadius+"px)"
                                                        });
                                                }
                                        });
                                        $("#backcover h1").animate({"top": "-2%", "opacity": "1"}, 3000);
                                        $("#downarrow").animate({"bottom": "10%"}, 3000);
                                        $("#downarrow").hover(function(){
                                                $(this).animate({"opacity": "0.8"}, 100);
                                        }, function(){
                                                $(this).animate({"opacity": "0.5"}, 500);
                                        });
                                        
                                        $("#gotoclubhub").animate({"right": "-25.5%", "opacity": "0.5"}, 2000);
                                        $("#gotoclubhub").hover(function(){
                                                $(this).animate({"right": "-15%", "opacity": "1"}, 200);
                                        }, function() {
                                                $(this).animate({"right": "-25.5%", "opacity": "0.5"}, 500);
                                        });
                                }
                                else
                                {
                                        $("#frontcover").css({"opacity":"1"});
                                        if(height>width)
                                        { 
                                                $("#frontcover").css({"background-size": "auto "+height*1.1+"px"});
                                        }
                                        else
                                        {
                                                $("#frontcover").css({"background-size": "110%"});
                                        }
                                        $('#frontcover').css({
                                                                "-webkit-filter": "blur(5px)",
                                                                "filter": "blur(5px)"
                                        });
                                        $("#backcover h1").css({"top": "-2%", "opacity": "1"});
                                        $("#downarrow").css({"bottom": "10%"});
                                        $("#downarrow").hover(function(){
                                                $(this).animate({"opacity": "0.8"}, 100);
                                        }, function(){
                                                $(this).animate({"opacity": "0.5"}, 500);
                                        });
                                        
                                        $("#gotoclubhub").css({"right": "-25.5%", "opacity": "0.5"});
                                        $("#gotoclubhub").css({"right": "-15%", "opacity": "1"});
                                        
                                }
                                
                                //Smooth scrolling sections
                                $(function() {
                                        $('a[href*=#]:not([href=#])').click(function() {
                                                if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
                                                        var target = $(this.hash);
                                                        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                                                        if (target.length) {
                                                                $('html,body').animate({
                                                                          scrollTop: target.offset().top
                                                                }, 1000);
                                                                return false;
                                                        }
                                                }
                                        });
                                });
                                
                                $(".fancybox").fancybox({
                                        beforeLoad: function() {
                                            this.title = $(this.element).attr('caption');
                                        },
                                        type: "image",
		                        openEffect: "elastic",
		                        closeEffect: "fade",
		                        helpers: {
                                                overlay: {
                                                        locked: false
                                                },
                                                title: {
		                                        type: "inside",
		                                },
                                        }
	                        });
                                
                                $(".mitem:not(.mignore)").each(function(index){
                                        var documentkeys = [
                                                "1xd0nS6OS8_YbgTslb2GIzozadXGEc4S3U2h07DrS5lw",
                                                "179CRNSypWoe7qdU-jrBXKPRGJiyNVnTDqz5h3wj-FyM",
                                                "1GL632tUnXJoemPjrjcXfqzgZ8R431sA_YalphF73M7g",
                                                "1s7-gNUrVBAbm1MosdKRjJVg0-ug-dw581EX5z4XIOsc",
                                                "1TajA7wJpYOacuKkEqgl3xIEI1QyrFsmnzehTdI4mZgY",
                                                "1itI9j__LOXQi5zDqBurP6F9FhnbdlgsSWxP-jp8WsQQ",
                                                "1lYTpk_Awe24ylL-9xOhnq1kiqSQcooafyBF66EFQVWM",
                                                "1ePLQ2agqPmZOmP2UVm3VHfiqif7-BtcWDVlGEKmlMm8",
                                                "1_d9liPLK261gtaYWF6mEFJ8lpdlY19ZFb-GNyb7xJb8",
                                                "1s8t1lcfUR7qtD2NxHzkNvlH1fw0rMfekLRNWe5-7nIo",
                                                "1X7H6YYhAFwz7LVPclG5TrW92MgPmajgTY_n1a4FmCi8"
                                        ]
                                        var value = $(this).attr("value");
                                        var folderroot = "https://googledrive.com/host/0BxhX9ArZ5oc0enlfYUFzdjVEN2M/"
                                        $(this).children("a").attr("href", folderroot+value);
                                        $(this).children("a").attr("title", "Click for more info.");
                                        $(this).children("a").attr("caption", "<iframe src=\"https://docs.google.com/document/d/"+documentkeys[index]+"/pub?embedded=true\" height=\""+$( window ).height()*0.8+"\"></iframe>ClubBox "+value+"");
                                        $(this).children("a").children("img").attr("src", folderroot+value);
                                        $(this).children("a").children("img").attr("alt", "The Next Big Thing");
                                        $(this).children("a").children("img").error(function(){
                                                $(this).attr("src", "/clubhub/media/ReserveAd1x1.png");
                                        });
                                        $(this).children("a").click(function(){
                                                console.log(value);
                                                _gaq.push(["_trackEvent", "ClubBox-Click", "Click", "ClubBox "+value]);
                                        });
                                });
                                
                        });     
